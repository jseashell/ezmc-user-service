generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id          Int     @id @default(autoincrement())
  username    String  @unique
  email       String  @unique
  firstName   String?
  lastName    String?
  phoneNumber String?
  locale      String?
}
